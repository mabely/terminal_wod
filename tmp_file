        <div class="wotd-item-headword__word">
          <h1 style="color: #6CD100">Samaritan</h1>
        </div>

                  <div class="wotd-item-headword__pronunciation">
            <div>
              [ s<span class="italic">uh</span>-<span class="bold">mar</span>-i-tn ]

                              <a
                  href="https://static.sfdict.com/audio/S00/S0058000.mp3"
                  class="wotd-item-headword__pronunciation-audio"
                ></a>
                          </div>
          </div>
        
        <div class="wotd-item-headword__pos-blocks">
                      <div class="wotd-item-headword__pos">
                <p>
                  <span class="italic">
                    <span class="luna-pos">noun</span>                  </span>
                </p>

                
                <p>one who is compassionate and helpful to a person in distress.</p>            </div>
                  </div>

