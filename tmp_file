        var keys = Object.keys(slotDefines.slots);
        var result = [];
        for (var i = 0; i < keys.length; i++) {
          result.push(slotDefines.slots[keys[i]]);
        }
        return result;
      };

      var destroySlots = function() {
        var slots = getSlots();
        if (!slots || !slots.length) {
          return;
        }
        googletag.destroySlots(slots);
      }

      var refreshSlots = function() {
        googletag.pubads().refresh();
      }

      var enableService = function() {
        googletag.pubads().enableSingleRequest();
        googletag.pubads().setTargeting('ptype', ['editorial']);
        googletag.pubads().disableInitialLoad();
        googletag.enableServices();
      }

      var current_size = false;


      var switchADs = function( force ) {
        var force = typeof force === "undefined" ? false : Boolean(force);

        var initialized = Boolean(current_size);
        if ( ! current_size || force === true ) {
          current_size = getCurrentDevice();
        } else if ( current_size !== getCurrentDevice() ) {
          current_size = getCurrentDevice();
        } else {
          return;
        }

        if ( initialized ) {
          destroySlots();
        }

        switch( current_size ) {
          case 'mobile':
            initializeMobile();
            break;

          case 'tablet':
            initializeTablet();
            break;

          default:
            initializeDesktop();
        }

        enableService();

        if ( initialized ) {
          refreshSlots();
        }
      }

      // enable ads
      switchADs();

      var initRefreshListener = function() {
        // checks if a reload ads has been triggered and actually switches the ads
        // it's triggered when the slideshow changes slides
        setInterval(function() {
          if ( ! window.crbReloadAds ) {
            return;
          }

          window.crbReloadAds = false;
          switchADs( true );
        }, 300);
      };


      var interval = false;
      var adsIntervalFn = function() {
        switchADs( true );
      }
      var adsIntervalReset = function() {
        if ( ! googletagCanAutoReload ) {
          return;
        }

        clearInterval(interval);
        interval = setInterval(
          adsIntervalFn,
          20 * 1000
        );
      }
      adsIntervalReset();

      if ( ! googletagCanAutoReload ) {
        initRefreshListener();
      }

      var timeout = false;
      jQuery(window).resize(function() {
        clearTimeout(timeout);
        timeout = setTimeout(function() {
          adsIntervalReset();
          switchADs();
        }, 500);
      });
    };

    googletag.cmd.push(dcomAdImplementation);

    googletag.cmd.push(function() {
      googletag.display('crb-ad-slot-top');
      googletag.display('crb-ad-slot-middle');
      googletag.display('crb-ad-slot-aside');
      googletag.display('crb-ad-slot-bottom');
    });
  })();

  googletag.cmd.push(function() {
    apstag.fetchBids({
      slots: slotConfigs[getCurrentDevice()]
    }, function(bids) {
      /*Callback method that triggers the first DFP request for googletag's
      disableInitialLoad integration after bids have been set*/

      apstag.setDisplayBids();
      window.OWT.notifyExternalBiddingComplete();

      /*
        Determine IAB consent if necessary, and trigger GPT ad refresh
        if consent is provided.
      */
      if(isEU) {
        try {
          if (typeof __cmp === 'function') {
            __cmp("getVendorConsents", {}, function(c) {
            var consent = false;
            if (typeof c.purposeConsents === 'array') {
              consent = c.purposeConsents.includes('3:true');
            } else if (typeof c.purposeConsents === 'object') {
              consent = c.purposeConsents[3];
            }
            setGPTPersonalization(consent);
            });
          } else {
            setGPTPersonalization(false);
          }
        } catch (error) {
          console.error('personalization: ', error)
          setGPTPersonalization(false);
        }
      } else {
        setGPTPersonalization(true);
      }
    });
  });

  googletag.cmd.push(function() {
    if(!window.googletag || !googletag.pubadsReady) {
      return;
    }

    var repositionPlayers = function() {
      console.log('Repositioning video players after ad slot loaded');
      window.crb_video_float_and_park.repositionPlayers();
    };

    googletag.pubads().addEventListener('slotOnload', function(event) {
      if(window.crb_video_float_and_park && window.crb_video_float_and_park.repositionPlayers) {
        repositionPlayers();
        return;
      }

      var $body = document.querySelector('body');
      var handler = function() {
        repositionPlayers();
        $body.removeEventListener('crb_float_and_park_ready', handler);
      };
      $body.addEventListener('crb_float_and_park_ready', handler);
    });
  });
})();
</script>
<script type="text/template" id="crb-wotd-nav-template">
<%
var renderItem = function(item, className) {
  var classes = [ className ];
  if (item.getTitle().length > 8) {
    classes.push('small');
  }

  var style = "color: " + item.getColor();
  %>
  <div class="wotd-widget__nav__link">
    <a
        href="<%= item.getUrl() %>"
        data-name="<%= item.getName() %>"
        class="<%= classes.join(' ') %>"
        style="<%= style %>"
    >
      <span><%= item.getDate() %></span>
      <%= item.getTitle() %>
    </a>
  </div>
<%
};

if (nextItem) {
  renderItem(nextItem, 'wotd-widget__nav__next');
}

if (prevItem) {
  renderItem(prevItem, 'wotd-widget__nav__prev');
}
%>
</script>
